<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Seguridad">

    <Button
        android:id="@+id/btnActualizarS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="Actualizar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.899"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="350dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:text="Ahora tu cuenta esta mas  protegida y todas las funciones de la app estan habilitadas"
        android:textColor="#676767"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView25" />

    <EditText
        android:id="@+id/txtPinS"
        android:layout_width="352dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="text"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.525"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tctContraseñaS"
        app:layout_constraintVertical_bias="0.106" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="143dp"
        android:layout_height="19dp"
        android:layout_marginBottom="4dp"
        android:text="Cambiar contraseña"
        android:textColor="#252525"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tctContraseñaS"
        app:layout_constraintEnd_toStartOf="@+id/lblContraseñaS"
        app:layout_constraintHorizontal_bias="0.177"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView22"
        app:srcCompat="@drawable/cheque" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="16dp"
        android:text="Pin de tu cuenta Nu"
        android:textColor="#252525"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/txtPinS"
        app:layout_constraintEnd_toStartOf="@+id/lblPinS"
        app:layout_constraintHorizontal_bias="0.029"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/lblPinS"
        android:layout_width="54dp"
        android:layout_height="21dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="12dp"
        android:text="123456"
        android:textColor="#676767"
        app:layout_constraintBottom_toTopOf="@+id/txtPinS"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/lblContraseñaS"
        android:layout_width="69dp"
        android:layout_height="20dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="4dp"
        android:text="Robin123"
        android:textColor="#676767"
        app:layout_constraintBottom_toTopOf="@+id/tctContraseñaS"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/tctContraseñaS"
        android:layout_width="352dp"
        android:layout_height="44dp"
        android:layout_marginTop="108dp"
        android:ems="10"
        android:inputType="text"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.525"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="149dp"
        android:layout_height="0dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="68dp"
        android:text="Dispositivo Autorizado"
        android:textColor="#252525"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.005"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView22" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="156dp"
        android:text="Seguridad"
        android:textColor="#1B1A1A"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btnVolverS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="52dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/textView22"
        app:layout_constraintHorizontal_bias="0.029"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/atras" />

</androidx.constraintlayout.widget.ConstraintLayout>